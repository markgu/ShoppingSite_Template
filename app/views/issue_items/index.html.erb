<h1>Listing issue_items</h1>

<table>
  <thead>
    <tr>
      <th>Comment</th>
      <th>Startdate</th>
      <th>Finishdate</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @issue_items.each do |issue_item| %>
      <tr>
        <td><%= issue_item.comment %></td>
        <td><%= issue_item.startDate %></td>
        <td><%= issue_item.finishDate %></td>
        <td><%= link_to 'Show', issue_item %></td>
        <td><%= link_to 'Edit', edit_issue_item_path(issue_item) %></td>
        <td><%= link_to 'Destroy', issue_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Issue item', new_issue_item_path %>
