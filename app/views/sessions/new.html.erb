<%= form_tag( {controller: "sessions" , action: "create"} , method: "post") do %>
    <div class="field">
      <%= label_tag :userId, "UserId" %><br>
      <%= text_field_tag :userId %>
    </div>
    <div class="field">
      <%= label_tag :password, "Password" %><br>
      <%= password_field_tag :password %>
    </div>
    <div class="actions">
      <%= submit_tag("Log on") %>
    </div>
<% end %>
